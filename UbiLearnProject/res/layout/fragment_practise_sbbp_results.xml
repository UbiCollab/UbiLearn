<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:focusable="false"
    android:orientation="vertical" >

    <RelativeLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_margin="3dp"
        android:layout_weight="0.33"
        android:background="@drawable/border" >

        <TextView
            android:id="@+id/testName1"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignParentLeft="true"
            android:layout_alignParentTop="true"
            android:inputType="none"
            android:text="Gangtest:" />

        <TextView
            android:id="@+id/testDate1"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignParentLeft="true"
            android:layout_below="@+id/testName1"
            android:inputType="none"
            android:singleLine="true"
            android:text="Dato: 22.05.1991" />
        
        	<TextView
        	    android:id="@+id/resultsDivider1"
        	    android:layout_width="wrap_content"
        	    android:layout_height="wrap_content"
        	    android:layout_alignParentLeft="true"
        	    android:layout_below="@+id/testDate1"
        	    android:text="Resultat:"
        	    android:textStyle="bold" />
        	
        	<TextView
            android:id="@+id/time1"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
           	android:layout_alignParentLeft="true"
            android:layout_below="@+id/resultsDivider1"
            android:text="Time: "
           />
        	
        	<TextView
        	    android:id="@+id/total1"
        	    android:layout_width="wrap_content"
        	    android:layout_height="wrap_content"
        	    android:layout_alignParentLeft="true"
        	    android:layout_below="@+id/time1"
        	    android:text="Score: " />

    </RelativeLayout>

    <RelativeLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_margin="3dp"
        android:layout_weight="0.33"
        android:background="@drawable/border" >

        <TextView
            android:id="@+id/testName2"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignParentLeft="true"
            android:layout_alignParentTop="true"
            android:inputType="none"
            android:text="Balansetest:" />

        <TextView
            android:id="@+id/testDate2"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignParentLeft="true"
            android:layout_below="@+id/TestName2"
            android:inputType="none"
            android:singleLine="true"
            android:text="Dato: 22.05.1991" />

        <TextView
            android:id="@+id/resultsDivider2"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignParentLeft="true"
            android:layout_below="@+id/testDate2"
            android:text="Resultat:"
            android:textStyle="bold" />

        <LinearLayout
            android:id="@+id/pairLay"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_alignParentLeft="true"
            android:layout_below="@+id/resultsDivider2"
            android:layout_margin="8dp"
            android:orientation="horizontal" >

            <TextView
                android:id="@+id/scoreLabel1"
                android:layout_width="150dp"
                android:layout_height="wrap_content"
                android:text="Samlede fÃ¸tter:"
                android:textAppearance="?android:attr/textAppearanceMedium" />

            <TextView
                android:id="@+id/score1"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="-"
                android:textAppearance="?android:attr/textAppearanceMedium" />
        </LinearLayout>

        <LinearLayout
            android:id="@+id/semiLay"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_alignParentLeft="true"
            android:layout_below="@+id/pairLay"
            android:layout_margin="8dp"
            android:orientation="horizontal" >

            <TextView
                android:id="@+id/scoreLabel2"
                android:layout_width="150dp"
                android:layout_height="wrap_content"
                android:text="Semi-tandem:"
                android:textAppearance="?android:attr/textAppearanceMedium" />

            <TextView
                android:id="@+id/score2"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="-"
                android:textAppearance="?android:attr/textAppearanceMedium" />
        </LinearLayout>

        <LinearLayout
            android:id="@+id/tandLay"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_alignParentLeft="true"
            android:layout_below="@+id/semiLay"
            android:layout_margin="8dp"
            android:orientation="horizontal" >

            <TextView
                android:id="@+id/scoreLabel3"
                android:layout_width="150dp"
                android:layout_height="wrap_content"
                android:text="Tandem:"
                android:textAppearance="?android:attr/textAppearanceMedium" />

            <TextView
                android:id="@+id/score3"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="-"
                android:textAppearance="?android:attr/textAppearanceMedium" />
        </LinearLayout>

        <LinearLayout
            android:id="@+id/totalLay"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_alignParentLeft="true"
            android:layout_below="@+id/tandLay"
            android:layout_margin="8dp"
            android:orientation="horizontal" >

            <TextView
                android:id="@+id/scoreLabel4"
                android:layout_width="150dp"
                android:layout_height="wrap_content"
                android:text="Totalt:"
                android:textAppearance="?android:attr/textAppearanceMedium" />

            <TextView
                android:id="@+id/total2"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="0 poeng"
                android:textAppearance="?android:attr/textAppearanceMedium" />
        </LinearLayout>
    </RelativeLayout>

    <RelativeLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_margin="3dp"
        android:layout_weight="0.33"
        android:background="@drawable/border" >

        <TextView
            android:id="@+id/testName3"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignParentLeft="true"
            android:layout_alignParentTop="true"
            android:inputType="none"
            android:text="Gangtest:" />

        <TextView
            android:id="@+id/testDate3"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignParentLeft="true"
            android:layout_below="@+id/testName3"
            android:inputType="none"
            android:singleLine="true"
            android:text="Dato: 22.05.1991" />

        <TextView
            android:id="@+id/resultsDivider3"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignParentLeft="true"
            android:layout_below="@+id/testDate3"
            android:text="Resultat:"
            android:textStyle="bold" />

        <TextView
            android:id="@+id/time3"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignParentLeft="true"
            android:layout_below="@+id/resultsDivider3"
            android:text="Time: " />

        <TextView
            android:id="@+id/total3"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignParentLeft="true"
            android:layout_below="@+id/time3"
            android:text="Score: " />
    </RelativeLayout>

</LinearLayout>